			<div class="tab-content">
               <div id="home" class="tab-pane fade in active">
                  <form action="#">
			{{!-- <form action="#" method="post"> --}}
				<div class="left-agileits-w3layouts same">
					<div class="gaps">
					
						<p>Doctor Name</p>
						{{input type="text" id="doctorname" name="First Name" valueBinding="doctorname" placeholder="Doctor Name" }}
					</div>	
					<div class="gaps">
						<p>Last Name</p>
							{{input type="text" id="lastname" name="Last Name" valueBinding="lastname" placeholder="Last Name" }}
					</div>
					<div class="gaps">
						<p>Gender</p>	
							<select class="form-control">
								<option></option>
								<option>Male</option>
								<option>Female</option>
							</select>
					</div>
					<div class="gaps">
						<p>Date of Birth</p>		
						{{input  id="datepicker1" name="Text" type="text" valueBinding="dob" onfocus="this.value = '';" onblur="if (this.value == '') {this.value = 'mm/dd/yyyy';}" required=""}}
					</div>
					 <div class="gaps">
						<p>Experience in Years</p>
						{{input type="text"id="experience" name="First Name" valueBinding="experience"placeholder="Experience" required=""}}
					</div>	
					<div class="gaps">
						<p>Designation</p>
						{{input type="text" id="Designation" name="First Name" valueBinding="designation"placeholder="Designation" required=""}}
					</div>			
					
					<div class="gaps">	
						<p>Phone Number</p>
						{{input type="text" id="Phone" name="Number" valueBinding="phonenumber"placeholder="Phone Number" required=""}}
					</div>
				<div class="gaps">	
						<p>Email</p>
						 {{input type="email"  model=this  valueBinding='email' placeholder="Email Id" required=true}}
					</div>
				</div>
				<div class="right-agileinfo same">
					<div class="gaps">
						<p>Hospital Name</p>
						{{input type="text" id="Hospital" name="First Name" valueBinding="hospitalname" placeholder="hospitalname" required=""}}
					</div>
					<div class="gaps">
						<p>Qualification</p>
						{{input type="text" id="Qualification" name="First Name"valueBinding="qualification" placeholder="qualification" required=""}}
					</div>
					<div class="gaps">
						<p>Address</p>
						{{textarea id="message" id="Address" name="message" valueBinding="address"placeholder="address" title="Please enter Your Comments"}}
						</div>
						<br><br>
					 <div class="gaps">
						<p>License id</p>
						{{input type="text" id="License" name="First Name" valueBinding="licenseid"placeholder="Licenseid" required=""}}
					</div>
					<div class="gaps">	
						<p>Password</p>
					{{input type="email"  model=this  valueBinding='password' placeholder="password" required=""}}
					</div>
				<div class="gaps">	
						<p>Confirm Password</p>
						 {{input type="email"  model=this  valueBinding='confirmpassword' placeholder="confirmpassword" required="true"}}
					</div>
				</div>
				<div class="clear"></div>
				<button type="submit" {{action "DocSubmit"}} class="pull-right btn btn-block btn-success">Submit</button>
			</form>
		</div>
   
   
